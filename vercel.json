{
        "version": 2,
        "builds": [
          {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": { "distDir": "dist/public" }
          },
          { "src": "dist/index.cjs", "use": "@vercel/node" }
       ],
       "routes": [
         {
           "handle": "filesystem"
         },
         {
           "src": "/(.*)",
           "dest": "dist/index.cjs"
         }
       ],
       "redirects": [
        {
          "source": "/(.*)",
          "destination": "https://www.findmyfuel.site/$1",
          "permanent": true
        }
      ]
    }